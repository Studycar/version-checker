{
  "name": "aps",
  "version": "8.2.3",
  "description": "ng-zorro-antd admin panel front-end framework",
  "author": "clj",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng serve -c=hmr --port 4208 --proxy-config proxy.config.js --open",
    "start:local": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng serve -c=hmr --port 4208 --proxy-config proxy.config.local.js --open",
    "hmr": "ng serve -c=hmr",
    "build": "rimraf ./dist & node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --aot=true --build-optimizer=true",
    "build:staging": "rimraf ./dist & node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=staging",
    "deploy:staging": "node scripts/deploy.js",
    "dev": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage --watch=false"
  },
